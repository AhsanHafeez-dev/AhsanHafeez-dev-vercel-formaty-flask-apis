{
    "templateName": "APA7",
    "documentclass": "apa7",
    "document_options": [
        "stu",
        "12pt",
        "floatsintext"
    ],
    "fontenc": null,
    "inputenc": null,
    "lmodern": false,
    "textcomp": false,
    "rawPreamables": [
        "r'\\DeclareLanguageMapping{american}{american-apa}'"
    ],
    "packages": [
        {
            "name": "babel",
            "options": [
                "american"
            ]
        },
        {
            "name": "float",
            "options": []
        },
        {
            "name": "lastpage",
            "options": []
        },
        {
            "name": "graphicx",
            "options": []
        },
        {
            "name": "xurl",
            "options": []
        },
        {
            "name": "hhline",
            "options": []
        },
        {
            "name": "multirow",
            "options": []
        },
        {
            "name": "fontenc",
            "options": [
                "T1"
            ]
        },
        {
            "name": "csquotes",
            "options": []
        },
        {
            "name": "biblatex",
            "options": [
                "style=apa",
                "sortcites=true",
                "sorting=nyt",
                "backend=biber"
            ]
        },
        {
            "name": "mathptmx",
            "options": []
        }
    ],
    "keywords": [
        "APA7",
        "journal",
        "stu",
        "article"
    ],
    "description": "The APA (American Psychological Association) 7th edition, commonly referred to as APA7, is a set of guidelines for writing and formatting research papers,",
    "publisher":"APA7",
    "format":"APA7",
    "type":"APA7"
}





